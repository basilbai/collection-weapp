.container {
  background-color: rgba(247, 248, 250, 1);
  height: 100vh;
  display: flex;
  align-items: center;
  padding-top: 200rpx;
}

.item {
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 640rpx;
  height: 130rpx;
  padding: 0 30rpx;
  background-color: #ffffff;

  box-shadow: 0px 3rpx 16rpx 0px rgba(255, 255, 255, 0.37);

}

.title {
  color: #333333;
  font-weight: bold;
  font-size: 28rpx;
}

.subTitle {
  margin-top: 10rpx;
  color: #666666;
  font-size: 24rpx;
}